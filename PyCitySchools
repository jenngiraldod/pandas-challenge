    "# Set passing threshold\n",
    "passing_score = 70\n",
    "\n",
    "# Calculate passing rates for reading and math by school\n",
    "school_performance = school_data_complete.groupby(\"school_name\").apply(\n",
    "    lambda x: pd.Series({\n",
    "        \"reading_pass_rate\": (x[\"reading_score\"] >= passing_score).mean() * 100,\n",
    "        \"math_pass_rate\": (x[\"math_score\"] >= passing_score).mean() * 100\n",
    "    })\n",
    ").sort_values(by=\"reading_pass_rate\", ascending=False)\n",
    "\n",
    "school_performance"
   ]
